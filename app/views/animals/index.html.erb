<h1>Welcome to the Animals Page!</h1>

<%= link_to 'Submit New Animal', new_animal_path %>

<ul>
    <% if @animal %>
        <% @animal.each do |animal| %>
        <li><%= link_to animal.name, animal_path(animal) %> <%= link_to "Edit Animal", edit_animal_path(animal) %> <%= link_to "Delete Animal", animal_path(animal), method: :delete %></li>
        <% end %>
    <% end %>
</ul>